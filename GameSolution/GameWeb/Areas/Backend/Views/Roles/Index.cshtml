@model IEnumerable<IdentityRole>

<table class="table table-striped table-bordered">
    <thead>
        <tr><th>Nom</th></tr>
    </thead>
    <tbody>

        @foreach(var role in Model)
            {
                <tr>
                    <td>@role.Name</td>
                </tr>
            }
    </tbody>
</table>

<a asp-action="Create">Ajouter un role</a>
